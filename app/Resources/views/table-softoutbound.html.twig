    <tr>
        <th>{{ 'isEmail'|trans }}</th>
        <td>
            {% if softmain.softOutbound.isEmail is same as(true) %}
                <i class="small material-icons green-text">check</i>
            {% elseif softmain.softOutbound.isEmail is same as(false) %}
                <i class="small material-icons red-text">close</i>
            {% elseif softmain.softOutbound.isEmail is same as(null) %}
                <p class="center-align">NC</p>
            {% endif %}
        </td>

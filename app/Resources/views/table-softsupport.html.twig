
<table class="striped">
    <tbody>
    <tr>
        <th>{{ 'isEmailSupport'|trans }}</th>
        <td>
            {% if softmain.softSupport.isEmailSupport  == false %}
                <i class="small material-icons red-text">close</i>
            {% elseif softmain.softSupport.isEmailSupport == true %}
                <i class="small material-icons green-text">check</i>
            {% else %}
                <p class="center-align">Non communiqué</p>
            {% endif %}
        </td>
    </tr>

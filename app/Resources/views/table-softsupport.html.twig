
<table class="striped">
    <tbody>
    <tr>
        <th>{{ 'isEmailSupport'|trans }}</th>
        <td>
            {% if softmain.softSupport.isEmailSupport is same as(true) %}
                <i class="small material-icons green-text">check</i>
            {% elseif softmain.softSupport.isEmailSupport is same as(false) %}
                <i class="small material-icons red-text">close</i>
            {% elseif softmain.softSupport.isEmailSupport is same as(null) %}
                <p class="center-align">NC</p>
            {% endif %}
        </td>
    </tr>
    <tr>
        <th>{{ 'isPhoneSupport'|trans }}</th>

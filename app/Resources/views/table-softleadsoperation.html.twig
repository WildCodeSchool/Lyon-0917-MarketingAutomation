    <tr>
        <th>{{ 'isContactObject'|trans }}</th>
        <td>
            {% if softmain.softLeadsOperation.isContactObject is same as(true) %}
                <i class="small material-icons green-text">check</i>
            {% elseif softmain.softLeadsOperation.isContactObject is same as(false) %}
                <i class="small material-icons red-text">close</i>

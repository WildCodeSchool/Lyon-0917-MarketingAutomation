    <tr>
        <th>{{ 'isContactObject'|trans }}</th>
        <td>
            {% if softmain.softLeadsOperation.isContactObject == false %}
                <i class="small material-icons red-text">close</i>
            {% elseif softmain.softLeadsOperation.isContactObject == true %}
                <i class="small material-icons green-text">check</i>
            {% else %}
                <p class="center-align">Non communiqué</p>

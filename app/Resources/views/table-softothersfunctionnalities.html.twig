
<table class="striped">
    <tbody>
    <tr>
        <th>{{ 'isProviderEmailChoice'|trans }}</th>
        <td>
            {% if softmain.softOtherFunctionnalities.isProviderEmailChoice == false %}
                <i class="small material-icons red-text">close</i>
            {% elseif softmain.softOtherFunctionnalities.isProviderEmailChoice == true %}
                <i class="small material-icons green-text">check</i>
            {% else %}
                <p class="center-align">Non communiqué</p>
            {% endif %}
        </td>

    <tr>
        <th>{{ 'isProviderEmailChoice'|trans }}</th>
        <td>
            {% if softmain.softOtherFunctionnalities.isProviderEmailChoice is same as(true) %}
                <i class="small material-icons green-text">check</i>
            {% elseif softmain.softOtherFunctionnalities.isProviderEmailChoice is same as(false) %}
                <i class="small material-icons red-text">close</i>
            {% elseif softmain.softOtherFunctionnalities.isProviderEmailChoice is same as(null) %}
                <i class="small material-icons grey-text">help_outline</i>

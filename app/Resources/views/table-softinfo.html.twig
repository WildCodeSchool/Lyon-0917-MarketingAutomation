
<table class="striped">
    <tbody>
    <tr>
        <th>{{ 'name'|trans }}</th>
        <td>{{ softmain.name | raw }}</td>
    </tr>
    <tr>
        <th>{{ 'type'|trans }}</th>
        <td>{{ softmain.type | raw }}</td>
    </tr>
    <tr>
        <th>{{ 'advantages'|trans }}</th>
        <td>{{ softmain.advantages | raw }}</td>
    </tr>
    <tr>
        <th>{{ 'drawbacks'|trans }}</th>
        <td>{{ softmain.drawbacks | raw }}</td>
    </tr>
    <tr>
        <th>{{ 'rgpd'|trans }}</th>
        <td>
            {% if softmain.softInfo.rgpd == false %}
                <i class="small material-icons red-text">close</i>
            {% elseif softmain.softInfo.rgpd == true %}
                <i class="small material-icons green-text">check</i>
            {% else %}
                <p class="center-align">Non communiqu√©</p>
            {% endif %}
        </td>
    </tr>
    <tr>
        <th>{{ 'customers'|trans }}</th>
        <td>{{ softmain.softInfo.customers | raw }}</td>
    </tr>
    <tr>
        <th>{{ 'hostingCountry'|trans }}</th>
        <td>{{ softmain.softInfo.hostingCountry | raw }}</td>
    </tr>
    <tr>
        <th>{{ 'creationDate'|trans }}</th>
        <td>{{ softmain.softInfo.creationDate | raw }}</td>
    </tr>
    <tr>
        <th>{{ 'subscriptionCost'|trans }}</th>
        <td>{{ softmain.softInfo.subscriptionCost | raw }}</td>
    </tr>
    <tr>
        <th>{{ 'trainingCost'|trans }}</th>
        <td>{{ softmain.softInfo.trainingCost | raw }}</td>
    </tr>
    <tr>
        <th>{{ 'webSite'|trans }}</th>
        <td><a href="{{ softmain.softInfo.webSite | raw }}">{{ softmain.name  | raw }}</a></td>
    </tr>
    </tbody>
</table>
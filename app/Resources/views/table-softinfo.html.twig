
<table class="striped">
    <tbody>
    <tr>
        <th>{{ 'name'|trans }}</th>
        <td>{{ softmain.name }}</td>
    </tr>
    <tr>
        <th>{{ 'type'|trans }}</th>
        <td>{{ softmain.type }}</td>
    </tr>
    <tr>
        <th>{{ 'advantages'|trans }}</th>
        <td>{{ softmain.advantages }}</td>
    </tr>
    <tr>
        <th>{{ 'drawbacks'|trans }}</th>
        <td>{{ softmain.drawbacks }}</td>
    </tr>
    <tr>
        <th>{{ 'rgpd'|trans }}</th>
        <td>
            {% if softmain.softInfo.rgpd == false %}
                <i class="small material-icons red-text">close</i>
            {% elseif softmain.softInfo.rgpd == true %}
                <i class="small material-icons green-text">check</i>
            {% else %}
                <p class="center-align">Non communiqu√©</p>
            {% endif %}
        </td>
    </tr>
    <tr>
        <th>{{ 'customers'|trans }}</th>
        <td>{{ softmain.softInfo.customers }}</td>
    </tr>
    <tr>
        <th>{{ 'hostingCountry'|trans }}</th>
        <td>{{ softmain.softInfo.hostingCountry }}</td>
    </tr>
    <tr>
        <th>{{ 'creationDate'|trans }}</th>
        <td>{{ softmain.softInfo.creationDate }}</td>
    </tr>
    <tr>
        <th>{{ 'subscriptionCost'|trans }}</th>
        <td>{{ softmain.softInfo.subscriptionCost }}</td>
    </tr>
    <tr>
        <th>{{ 'trainingCost'|trans }}</th>
        <td>{{ softmain.softInfo.trainingCost }}</td>
    </tr>
    <tr>
        <th>{{ 'webSite'|trans }}</th>
        <td><a href="{{ softmain.softInfo.webSite }}">{{ softmain.name }}</a></td>
    </tr>
    </tbody>
</table>